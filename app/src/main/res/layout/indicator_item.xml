<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content"
	android:orientation="horizontal"
	android:layout_margin="@dimen/xs_margin"
    android:layout_height="wrap_content">

	<ImageView
		android:id="@+id/iconIndicator"
		android:layout_width="@dimen/l_margin"
		android:layout_height="@dimen/l_margin"
		android:src="@drawable/ic_launcher_foreground"/>
	<TextView
		android:id="@+id/nameIndicator"
		style="@style/TextView10spBold"
		android:layout_width="wrap_content"
		android:textColor="@color/black"
		android:layout_marginLeft="@dimen/s_margin"
		android:layout_height="match_parent"
		android:gravity="center"
		android:text="Reksadana"/>
</LinearLayout>
